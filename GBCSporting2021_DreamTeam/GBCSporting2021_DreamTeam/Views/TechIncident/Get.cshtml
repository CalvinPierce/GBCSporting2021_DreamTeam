@model Incident
@{
    ViewBag.Title = "Get Technician";
}

<h2>Get Technician</h2>
<form asp-action="List" method="post">

    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <select asp-for="TechnicianId" class="form-control">
            <option value=""></option>
            @foreach (Technician t in ViewBag.Technician)
            {
                <option value="@t.TechnicianId">@t.Name</option>
            }
        </select>
    </div>

    <button type="submit" class="btn btn-primary">Select</button>
    <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Cancel</a>
</form>